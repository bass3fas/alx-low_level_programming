#include "main.h"
/**
 * print_number - go
 * @n: number
 */


void print_number(int n)
{
	int c, m, num;

	c = 1;
	m = 1;
	num = n;

	if (num < 0)
	{
		_putchar('-');
		num = num * -1;
	}
	while (num / (m * 10) > 0)
		m = m * 10;

	while (num >= 0)
	{
		if (m == 1)
		{
			_putchar(num % 10 + '0');
			num = -1;
		}
		else
		{
			_putchar((num / m % 10) + '0'));
		
}
