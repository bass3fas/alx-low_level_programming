#include "main.h"

/**
 * jack_bauer - prints every minute of a day
 * Return : nothing
 */


void jack_bauer(void)
	{
		int i, j, n, l, k;

		n = 9;

		for (i = 0; i < 3; i++)
		{
			if (i == 2)
				n = 4;

			for (j = 0; j < n; j++)
			{
				for (k = 0; k < 6; k++)
				{
					for (l = 0; l < 10; l++)
					{
						_putchar('0' + i);
						_putchar('0' + j);
						_putchar(':');
						_putchar('0' + k);
						_putchar('0' + l);
						_putchar('\n');
					}
				}
			}
		}
	}
